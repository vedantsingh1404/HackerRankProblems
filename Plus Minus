#include<iostream>

using namespace std;

int main(){
    double pos = 0, neg = 0, zero = 0;
    int n;
    cin>>n;
    for(int i = 0;i<n;i++){
        int x;
        cin>>x;
        if(x>0) pos++;
        else if(x<0) neg++;
        else zero++;
        
    }
    
    cout<<pos/n<<endl<<neg/n<<endl<<zero/n;
}
